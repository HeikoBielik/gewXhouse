within gewXhouse.Connectors.Interfaces;

connector HouseInput

  parameter Integer N = 6;
  input Models.Surface surfaces[N];
  input Models.Surface floor "Floor";
  input Modelica.SIunits.Area total "Total surface";
  input Modelica.SIunits.Volume volume "Inner volume of the house";
  
  annotation(
    Icon(graphics = {Rectangle(fillPattern = FillPattern.Solid, extent = {{-100, 100}, {100, -100}})}));
end HouseInput;
